{
    "manifest_version": 3,
    "name": "Effortless Subject Reordering",
    "version": "1.0",
    "description": "Convenient way to fill out OE/HULM Google Forms in IIT(BHU)",
    "icons": {
      "16":"icon_16.ico",
      "32":"icon_32.ico",
      "48":"icon_48.ico",
      "64":"icon_64.ico",
      "128": "icon_128.ico"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["jquery.min.js", "jquery-ui.min.js", "content.js"],
            "css": ["jquery-ui.min.css"]
        }
    ],
    "permissions":[
      "tabs"
    ]
  }